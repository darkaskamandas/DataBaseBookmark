    "freeCodeCamp",
    "Mongoose",
    "Express",
    "React",
    "Node",
    "Pinterest"
  ],
  "scripts": {
    "start": "npm-run-all --parallel watch:client watch:server",
    "deploy": "node dist/server.bundle.js",
    "build": "webpack --config webpack.config.babel.js",
